    </outlets>
    <displays/>
    <params/>
    <attribs/>
    <includes>
      <include>./noise.h</include>
    </includes>
    <code.declaration><![CDATA[struct noise_state state;]]></code.declaration>
    <code.init><![CDATA[noise_init(&state);]]></code.init>
    <code.krate><![CDATA[brown_noise(&state, outlet_wave);]]></code.krate>
  </obj.normal>
