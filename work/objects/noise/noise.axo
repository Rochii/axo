    <author>Jason Harris</author>
    <license>BSD</license>
    <inlets/>
    <outlets>
      <frac32buffer.bipolar name="wave" description="white noise"/>
    </outlets>
    <displays/>
    <params/>
    <attribs/>
    <includes>
      <include>./noise.h</include>
    </includes>
